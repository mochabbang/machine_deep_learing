<script type="text/javascript">
    //<![CDATA[
    // 추천검색어 5개
    var arrPopularWord = CommonGNBPopArray["POPULAR"];
    var _l_keyword = "";
    var _l_url = "";
    var strPopularWord = ""
    //alert(arrPopularWord.length);
    if (arrPopularWord.length > 0) {
        //rndPopularIdx = Math.floor(Math.random() * arrPopularWord.length);
        for (var i = 0; i < arrPopularWord.length; i++) {
            _l_keyword = arrPopularWord[i].msg;
            _l_url = arrPopularWord[i].url;

            if (location.href.toLowerCase().indexOf("new.alba.co.kr") > -1) {
                _l_url = _l_url.toLowerCase().replace("www.alba.co.kr", "new.alba.co.kr");
            }

            _l_url = _l_url.toLowerCase().replace("/wsearch/wsearch.asp", "/search/search.asp");

            if (_l_url.toLowerCase().indexOf("section=") == -1 && _l_url.toLowerCase().indexOf("wssrchword=") > -1) {
                if (_l_url.toLowerCase().indexOf("?wssrchword=")) {
                    _l_url = _l_url.toLowerCase().replace("?wssrchword=", "?section=ALL&wssrchword=");
                } else if (_l_url.toLowerCase().indexOf("&wssrchword=")) {
                    _l_url = _l_url.toLowerCase().replace("&wssrchword=", "&section=ALL&wssrchword=");
                }
            }

            strPopularWord += "<li><a href='" + _l_url + "' target='_blank'>" + _l_keyword + "</a></li>";
        }
    }

    // 디폴트 검색어
    var arrDefaultWord = CommonGNBPopArray["DEFAULT"];
    var _srchWord = "";
    var _srchUrl = "";
    var _srchTarget = "";
    var _srchType = "";
    var _srchUrl_old = "";
    if (arrDefaultWord.length > 0) {
        rndIdx = Math.floor(Math.random() * arrDefaultWord.length);
        _srchWord = arrDefaultWord[rndIdx].msg;
        _srchUrl = arrDefaultWord[rndIdx].url;
        _srchTarget = arrDefaultWord[rndIdx].target;
        _srchType = arrDefaultWord[rndIdx].type;
        _srchUrl_old = "";
    }

    var gnbFunc = {
        wsChkForm: function () {
            $("#wsSrchWord").val($("#wsSrchWord").val().trim());
            if ($.trim($("#wsSrchWord").val()) != "") this.clearSearchValue();
            if ($.trim($("#wsSrchWord").val()) == _srchWord || $("#wsSrchWord").css("backgroundImage") != "none" && _srchUrl != "") {
                if (_srchTarget == "_blank") {
                    window.open(_srchUrl);
                } else {
                    location.href = _srchUrl;
                }
                return false;
            }

            // _srchUrl 추후 적용 여부 결정
            if ($.trim($("#wsSrchWord").val()).length == 0) {
                alert("검색어를 입력해 주세요.");
                $("#wsSrchWord").focus();
                return false;
            } else {

                if ($.trim($("#wsSrchWord").val()).length > 20) {
                    alert("검색어는 20글자 이내로 입력해 주세요.");
                    $("#wsSrchWord").focus();
                    return false;
                }
            }

            var chkSrchWord = $("#wsSrchWord").val();

            //검색데이터팀 예외 특수문자
            chkSrchWord = chkSrchWord.replace(/,/gi, " ");
            chkSrchWord = chkSrchWord.replace(/\//gi, " ");
            chkSrchWord = chkSrchWord.replace(/\|/gi, " ");
            chkSrchWord = chkSrchWord.replace(/\\/gi, " ");
            chkSrchWord = chkSrchWord.replace(/\?/gi, " ");
            chkSrchWord = chkSrchWord.replace(/·/gi, " ");
            chkSrchWord = chkSrchWord.replace(/ㆍ/gi, " ");

            chkSrchWord = chkSrchWord.replace(/\'/gi, "");
            chkSrchWord = chkSrchWord.replace(/\"/gi, "");

            $("#wsSrchWord").val(chkSrchWord);

            if ($.trim($("#Section").val()) != "") {
                $("#srchType").val($("#Section").val());
                $("#section").val($("#Section").val());
            } else {
                $("#Section").val("ALL");
            }
            return true;
        },

        setDefaultKeyword: function () {
            if ($.trim($("#wsSrchWord").val()) == "" && arrDefaultWord.length > 0) {
                if (_srchUrl_old != "")
                    _srchUrl = _srchUrl_old;
                if (_srchType == "text") {
                    $("#wsSrchWord").val(_srchWord);
                } /*
                    else if (_srchType == "image") {
                        $("#wsSrchWord").css("background","url(" + _srchWord + ") no-repeat center top");
                    } */
            }
            $("#PopularWord").html(strPopularWord);
        },

        clearSearchValue: function () {
            _srchUrl_old = _srchUrl;
            _srchUrl = "";
            if (_srchType == "text") {
                $("#wsSrchWord").val("");
            } /*
                else if (_srchType == "image") {
                    $("#wsSrchWord").css("backgroundImage","");
                } */
        },

        setPersonAlarmList: function (pagesize) {
            $("div.notifyLayer").html("<p class='loding'><img src='//image.alba.kr/common/loading.gif' border='0' alt='loading' title='loading' /></p>").show();
            $.ajax({
                url: "/person/notify/NotifyHeaderAjax.asp",
                type: "post",
                dataType: "html",
                cache: false,
                data: "pagesize=" + pagesize,
                success: function (data) {
                    $("div.notifyLayer").html(data);
                }
            });
        },

        setPersonAlarmCnt: function () {
            var intPersonAlarmCnt = 0;
            $.ajax({
                url: "/person/notify/NotifyHeaderCntAjax.asp",
                dataType: "text",
                cache: false,
                success: function (data) {
                    intPersonAlarmCnt = Number(data);
                    if (intPersonAlarmCnt > 0) {
                        $("div.notifyLayer").prev().addClass("newAlarm");
                    }
                    else {
                        $("div.notifyLayer").prev().removeClass("newAlarm");
                    }

                    if ($("#LNBPersonAlarmCnt").length > 0) {
                        if (intPersonAlarmCnt > 0) {
                            $("#LNBPersonAlarmCnt").removeClass("hide");
                            $("#LNBPersonAlarmCnt").html(intPersonAlarmCnt);
                        }
                        else {
                            $("#LNBPersonAlarmCnt").addClass("hide");
                        }
                    }

                    if ($("p.notifyCount").length > 0) {
                        setMainAlarmCnt(intPersonAlarmCnt);
                    }
                }
            });
        },

        setPersonAlarmMod: function (executegbn, alarmconfirm, idx) {
            if (confirm("확인 처리 하시겠습니까?")) {
                $.ajax({
                    url: "/person/notify/NotifyUptAjax.asp",
                    type: "post",
                    dataType: "text",
                    cache: false,
                    data: "executegbn=" + executegbn + "&alarmconfirm=" + alarmconfirm + "&chkalarm=" + idx,
                    success: function (result) {
                        if (result == "0") {
                            gnbFunc.setPersonAlarmList(20);
                            gnbFunc.setPersonAlarmCnt();

                            if ($("div.notifyContent").length > 0) {
                                notify.setList(20);
                            }
                            if (executegbn == "ALL") {
                                alert("모든 알림이 확인되었습니다.");
                            }
                            else {
                                alert("선택하신 알림이 확인되었습니다.");
                            }
                        }
                        else {
                            alert("오류가 발생했습니다.");
                        }
                    }
                });
            }
        }
    };
    //]]>
</script>

