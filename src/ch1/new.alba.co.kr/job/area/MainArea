    <script type="text/javascript">
    // 리스트 높이값 ( 가장 높은값으로 동일하게 적용 )
    var firstList = $(".topSelectList.seoul .areaList , .topSelectList.gyeonggi .areaList").eq(0).height();
    var secondList = $(".topSelectList.seoul .areaList , .topSelectList.gyeonggi .areaList").eq(1).height();
    var thirdList = $(".topSelectList.seoul .areaList , .topSelectList.gyeonggi .areaList").eq(2).height();
    var fourthList = $(".topSelectList.seoul .areaList , .topSelectList.gyeonggi .areaList").eq(3).height();
    var fifthList = $(".topSelectList.seoul .areaList , .topSelectList.gyeonggi .areaList").eq(4).height();
    var maxHeight = Math.max(firstList,secondList,thirdList,fourthList,fifthList);
    $(".topSelectList.seoul .areaList , .topSelectList.gyeonggi .areaList").css("height",maxHeight);

    $("#spanSido").html(getSidoName("031"));
    $("#spanViewType").html(getViewTypeName("A"));

    // 상세검색 열기 닫기
    $("#detailSearchBtn").click(function() {
	    if ($("#detailSearchBtn").hasClass("on")) {
		    $("#detailSearchBtn").removeClass("on");
		    $("#detailSearchText").text("상세 검색 열기");
		    $("#schDetail").slideUp("fast",function(){
			    $("#schDetail").hide();
		    });
		    $("#areaSchOpt").slideUp("fast", function(){
			    $("#areaSchOpt").hide();
		    });
	    } else {
		    $("#detailSearchBtn").addClass("on");
		    $("#detailSearchText").text("상세 검색 닫기");
		    $("#areaSchOpt").slideDown("fast");
		    $("#areaSchOpt").show();
		    $("#schDetail").slideDown("fast");
		    $("#schDetail").show();
		    // 근무기간, 근무요일, 근무시간 높이값 설정
		    $(".work dl .inputWrap").height(116);
	    }
	    $("#schDetailBtn").hide();
    });
    </script>


