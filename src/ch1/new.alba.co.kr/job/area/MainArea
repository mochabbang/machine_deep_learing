//상품소개영상 ----------------------------------------------------------------------------------------------------------------------
	//상품안내 - 상품소개영상
	$("#GoodsGuide").delegate(".product-guide__movie", "click", function() {
		var StrProcutNM = $(this).parent().text();
		var StrProcutName = StrProcutNM.replace("상품 소개 영상", "").trim();

		BA.Collect("EVENT", {
			"pageurl" : location.pathname
		,	"category1" : "상품안내"
		,	"category2" : "상품소개영상"
		,	"category3" : StrProcutName
		});
	});
	//상품안내 - 상품신청
	$("#GoodsGuide").delegate(".product-action__regist", "click", function() {
		var StrProcutNM = $(this).parents(".product-guide__item").find(".product-guide__name").text();
		var StrProcutName = StrProcutNM.replace("상품 소개 영상", "").trim();

		BA.Collect("EVENT", {
			"pageurl" : location.pathname
		,	"category1" : "상품안내"
		,	"category2" : "상품신청"
		,	"category3" : StrProcutName
		});
	});

	//상품선택 - 상품소개영상
	$("#ProductGuideAll").delegate(".product-guide__movie", "click", function() {
		var StrProcutNM = $(this).parent().text();
		var StrProcutName = StrProcutNM.replace("상품 소개 영상", "").trim();

		BA.Collect("EVENT", {
			"pageurl" : location.pathname
		,	"category1" : "상품선택"
		,	"category2" : "상품소개영상"
		,	"category3" : StrProcutName
		});
	});
	//상품선택 - 상세보기
	$("#ProductGuideAll").delegate(".product-action__view", "click", function() {
		if (!$(this).hasClass("on")) {
			var StrProcutNM = $(this).parents(".product-guide__item").find(".product-guide__name").text();
			var StrProcutName = StrProcutNM.replace("상품 소개 영상", "").trim();

			BA.Collect("EVENT", {
				"pageurl" : location.pathname
			,	"category1" : "상품선택"
			,	"category2" : "상세보기"
			,	"category3" : StrProcutName
			});
		}
	});
	//상품선택 - 상품담기
	$("#ProductGuideAll").delegate(".product-action__cart", "click", function() {
		var StrProcutNM = $(this).parents(".product-guide__item").find(".product-guide__name").text();
		var StrProcutName = StrProcutNM.replace("상품 소개 영상", "").trim();

		BA.Collect("EVENT", {
			"pageurl" : location.pathname
		,	"category1" : "상품선택"
		,	"category2" : "상품담기"
		,	"category3" : StrProcutName
		});
	})
</script>






