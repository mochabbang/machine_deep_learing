<script type="text/javascript">
    /*
        로그인전 confirm체크
        - goUrl : 로그인 후 가야할 페이지
        - loginType : 로그인타입 => 팝업로그인(loginPopup), 기업로그인(loginBiz), 개인로그인(loginPerson), 공통로그인(loginGate)
        - confirmType : confirm 메시지 => "" : confirm없음, "P" : 개인회원로그인메시지, "C" : 기업회원로그인메시지, "A" : 공통로그인메시지
        - targetObj : target 객체명
    */
    function loginConfirmCheck(goUrl, loginType, confirmType, targetObj) {
            
            var confirmMsg = "";
            if (confirmType == "P")
            {
                confirmMsg = "개인회원으로 로그인 후 이용 가능한 서비스입니다.\n지금 로그인 하시겠습니까?";
            }
            else if (confirmType == "C")
            {
                confirmMsg = "기업회원으로 로그인 후 이용 가능한 서비스입니다.\n지금 로그인 하시겠습니까?";
            }
            else if (confirmType == "A")
            {
                confirmMsg = "로그인 후 이용 가능한 서비스입니다. 지금 로그인 하시겠습니까?";
            }

            var usertype = "";

            if (confirmType == "P")
            {
                usertype = "P";
            }

            if (confirmMsg != "")
            {
                if (confirm(confirmMsg))
                {
                    eval(loginType + usertype + "('" + goUrl + "', '" + targetObj + "');");
                }
            }
            else
            {
                eval(loginType + usertype + "('" + goUrl + "', '" + targetObj + "');");
            }
            
		return false;
    }

    /* 팝업로그인 (디폴드기업) */
    function loginPopup(gourl, target) {
        var PopWith, PopHeight;
        PopWith = 470;
        PopHeight = 640;
        try {
                
                if (gourl == "" || gourl == null) {
                    if (target == "" || target == null) {
                        popup_open_login("https://" + location.host + "/auth/LoginPop.asp", "login", PopWith, PopHeight, "no");
                    } else {
                        popup_open_login("https://" + location.host + "/auth/LoginPop.asp?target=" + target, "login", PopWith, PopHeight, "no");
                    }
                } else {
                    if (target == "" || target == null) {
                        popup_open_login("https://" + location.host + "/auth/LoginPop.asp?gourl=" + encodeURIComponent(gourl), "login", PopWith, PopHeight, "no");
                    } else {
                        popup_open_login("https://" + location.host + "/auth/LoginPop.asp?gourl=" + encodeURIComponent(gourl) + "&target=" + target, "login", PopWith, PopHeight, "no");
                    }
                }
                
		} catch (e) {
            if (target == "" || target == null) {
                location.href = 'https://sign0.alba.co.kr/login/logingate.asp?site=WWW&usertype=C&gourl=' + encodeURIComponent(gourl);
            } else {
                target.location.href = 'https://sign0.alba.co.kr/login/logingate.asp?site=WWW&usertype=C&gourl=' + encodeURIComponent(gourl);
            }
        }
    }

    /* 개인팝업로그인 */
    function loginPopupP(gourl, target) {
        var PopWith, PopHeight;
        PopWith = 470;
        PopHeight = 640;
        try {
                
                if (gourl == "" || gourl == null) {
                    if (target == "" || target == null) {
                        popup_open_login("https://" + location.host + "/auth/LoginPop.asp?usertype=P", "login", PopWith, PopHeight, "no");
                    } else {
                        popup_open_login("https://" + location.host + "/auth/LoginPop.asp?usertype=P&target=" + target, "login", PopWith, PopHeight, "no");
                    }
                } else {
                    if (target == "" || target == null) {
                        popup_open_login("https://" + location.host + "/auth/LoginPop.asp?usertype=P&gourl=" + encodeURIComponent(gourl), "login", PopWith, PopHeight, "no");
                    } else {
                        popup_open_login("https://" + location.host + "/auth/LoginPop.asp?usertype=P&gourl=" + encodeURIComponent(gourl) + "&target=" + target, "login", PopWith, PopHeight, "no");
                    }
                }
                
		} catch (e) {
            if (target == "" || target == null) {
                location.href = 'https://sign0.alba.co.kr/login/logingate.asp?site=WWW&gourl=' + encodeURIComponent(gourl);
            } else {
                target.location.href = 'https://sign0.alba.co.kr/login/logingate.asp?site=WWW&gourl=' + encodeURIComponent(gourl);
            }
        }
    }

    /* 기업로그인 */
    function loginBiz(gourl, target) {
        var strTargetUrl = "";
        if (gourl == "" || gourl == null) {
            strTargetUrl = "/auth/LoginBiz.asp?gourl=https%3a%2f%2fnew.alba.co.kr%2fjob%2farea%2fmainarea%3fviewtype%3dA%26sidocd%3d031";
        } else {
            strTargetUrl = "/auth/LoginBiz.asp?gourl=" + encodeURIComponent(gourl);
        }
        if (target == "" || target == null) {
            location.href = strTargetUrl;
        } else {
            target.location.href = strTargetUrl;
        }
    }

    /* 개인로그인 */
    function loginPerson(gourl, target) {
        var strTargetUrl = "";
        if (gourl == "" || gourl == null) {
            strTargetUrl = "/auth/LoginPerson.asp?gourl=https%3a%2f%2fnew.alba.co.kr%2fjob%2farea%2fmainarea%3fviewtype%3dA%26sidocd%3d031";
        } else {
            strTargetUrl = "/auth/LoginPerson.asp?gourl=" + encodeURIComponent(gourl);
        }
        if (target == "" || target == null) {
            location.href = strTargetUrl;
        } else {
            target.location.href = strTargetUrl;
        }
    }

    /* 공통로그인 */
    function loginGate(gourl, target) {
        var strTargetUrl = "";
        if (gourl == "" || gourl == null) {
            strTargetUrl = "https://sign0.alba.co.kr/login/logingate.asp?site=WWW&gourl=https%3a%2f%2fnew.alba.co.kr%2fjob%2farea%2fmainarea%3fviewtype%3dA%26sidocd%3d031";
        } else {
            strTargetUrl = "https://sign0.alba.co.kr/login/logingate.asp?site=WWW&gourl=" + encodeURIComponent(gourl);
        }
        if (target == "" || target == null) {
            location.href = strTargetUrl;
        } else {
            target.location.href = strTargetUrl;
        }
    }

    /* 로그인팝업용 (가운데 정렬) */
    function popup_open_login(url, wname, w, h, scbar) {
        var top = (screen.availHeight - h) / 2;
        var left = (screen.availWidth - w) / 2;
        var prty = "top=" + top + ", left=" + left + ", width=" + w + ", height=" + h + ", status=0, resizable=0, scrollbars=" + scbar;
        window.open(url, wname, prty);
    }
</script>

