    <script type="text/javascript">
        $(".headerMenu>li>a").bind("mouseenter focus", function () {
            $(".headerMenu>li").removeClass("active");
            $(this).parent().addClass("active");
            $(".headerFilter").remove();
            if (!$(this).parent().hasClass("genius")) {
                $(this).siblings(".headerMenuFull").wrap("<div class='headerMenuFullWrap'></div>");
                $("body").prepend("<div class='headerFilter'></div>");
            }
        });
        $(".headerMenu").mouseleave(function () {
            $(".headerMenu>li").removeClass("active");
            $(".headerMenuFullWrap>.headerMenuFull").unwrap();
            $(".headerFilter").remove();
        });

        $("#Header a, #Header button, #Header iframe, .headerQuick a").focus(function () {
            $(".headerMenu>li").removeClass("active");
        });
    </script>
