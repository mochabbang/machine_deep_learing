    <span class="bottomLine"></span>
    <script type="text/javascript">
        // 리스트 초기셋팅
        $(".asideGroup").each(function () {
            var thisWrap = $(this);
            var thisList = thisWrap.find(".asideList>li");
            var thisHeight = thisList.outerHeight() * thisList.length

            // 버튼 유무처리
            if (thisList.length < 6) {
                thisWrap.find(".moreBtn").hide();
            } else {
                $(this).find(".asideList").css("height", "125px");
                thisWrap.find(".moreBtn").show();
            };
            // 지역 높이값 해제
            if (thisWrap.hasClass("local")) {
                $(this).find(".asideList").css("height", "auto");
                thisWrap.find(".moreBtn").hide();
            };
        });

        // 더보기 슬라이드
        $("#AreaAside .moreBtn").click(function () {
            var thisParent = $(this).parent();
            var listLen = thisParent.find(".asideList>li").length;
            var listHeight = thisParent.find(".asideList>li").height();
            var inList = thisParent.find(".inSlide>li>ul");
            var inListHeight = thisParent.find(".inSlide>li>ul").outerHeight();
            var animateVal = listLen * listHeight;

            // 움직임처리
            if ($(this).hasClass("on")) {
                $(this).removeClass("on");
                $(this).siblings(".asideList").stop().animate({
                    height: "125px"
                }, 300);
                $(this).text("더보기");
            } else {
                $(this).addClass("on");
                $(this).siblings(".asideList").stop().animate({
                    height: animateVal
                }, 300);
                $(this).text("닫기");
            }
            return false;
        });

        // 우측 AreaASid 영역
        var tableListHeight = $(".tableList").height();
        var areaAsideHeight = $("#AreaAside").height();
        if ($(".asideGroup").length > 0) {
            var areaLastTop = $(".asideGroup:last-child").offset().top;
        }

        function AreaASideMove() {
            //상단 고정노출기능 삭제 (2016-12-22 김지훈)
            /*$(window).scroll(function() {
                if ($(this).scrollTop() > areaLastTop) {
                    if ($("#schDetailBtn, #detailSearchBtn").hasClass("on")) {
                        $(".asideGroup:last-child").removeClass("lastFlow");
                    } else {
                        $(".asideGroup:last-child").addClass("lastFlow");
                    }
                } else {
                    $(".asideGroup:last-child").removeClass("lastFlow");
                }
            });*/
        }

        if (tableListHeight > areaAsideHeight) {
            AreaASideMove();
        } else {
            $(".asideGroup:last-child").css("border-bottom", "0");
        }

        $("#AreaAside .inSlide .localName").click(function () {
            var localHeight = $(this).next().children().height() * $(this).next().children().length;

            if ($(this).hasClass("open")) {
                $(".inSlide .localName").removeClass("open");
                $(".inSlide ul").stop().slideUp(200);
                $(this).parent().css("height", "inherit");
                if (tableListHeight > areaAsideHeight) {
                    areaLastTop = areaLastTop - localHeight;
                    AreaASideMove();
                }
            } else {
                $(".inSlide .localName").removeClass("open");
                $(".inSlide ul").slideUp(200);
                $(this).addClass("open");
                $(this).next().slideDown(400);
                $(this).parent().css("height", "auto");
                if (tableListHeight > areaAsideHeight) {
                    areaLastTop = areaLastTop + localHeight;
                    AreaASideMove();
                }
            }
            return false;
        });
    </script>
    <!-- // -->


                    <!-- 일반채용정보 카테고리별 카운트 -->
                </div>
        </div>
	</div>
	<!-- //subContents -->
</div>
