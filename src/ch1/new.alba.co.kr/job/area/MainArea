<script type="text/javascript">
	$(window).scroll(function () {	// 스크롤 늦게 적용되는 현상 떄문에 .stop() 추가 (2015-08-11)
		if ($(this).scrollTop() > 400) {
			$(".quickTop").stop().animate({ "bottom": "20px" }, 300);
		} else {
			$(".quickTop").stop().animate({ "bottom": -$(".quickTop").height() }, 100);
		}
	});

    $(function () {
        $(".quickTop").click(function () {
            $("body, html").animate({
                scrollTop: 0
            }, 0);
            return false;
        });

         $("#SkyBanner").delegate(".chat a", "click", function() {
            BA.Collect("EVENT", {
                "pageurl" : location.pathname
            ,	"category1" : "퀵메뉴"
            ,	"category2" : "채팅문의&지원"
            });
        });
    });
</script>

<!-- //배너(우측 skyBanner) -->
<script type="text/javascript">
    var headerFix = function () {
        //상단 Header fixed
        if ($("#HeaderMenuWrap").length > 0 || $("#HeaderMenuBizWrap").length > 0 || $(".brand-regist").length > 0) {
            var ScrHeader = $("#HeaderMenuWrap, #HeaderMenuBizWrap, .brand-regist").offset().top;
            var ScrWindow = $(window);
            var ScrBody = $("body");
            var timer;

            function ScrollHeader() {
                var ScrTop = $(this).scrollTop();

                if (ScrTop > ScrHeader) {
                    ScrBody.addClass("scroll");
                } else {
                    ScrBody.removeClass("scroll");
                }
            }

            ScrWindow.scroll(function () {
                if (timer) {
                    window.clearTimeout(timer);
                }

                timer = window.setTimeout(function () {
                    ScrollHeader();
                }, 10);
            });

            ScrollHeader();
        }
    }
    $(function () {
        headerFix();
    });
</script>



    
