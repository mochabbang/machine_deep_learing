<script type="text/javascript">
    //<![CDATA[
    showMyAdLayers("");

    function showMyAdLayers(uid) {
        var chkUrl = document.location.href;
        var settingPx = "-41px 0 0 400px;";
        if (chkUrl.toLowerCase().indexOf("/job/brand/") > -1) {	//브랜드 채용관
            settingPx = "-41px 0 0 312px;";
        } else if (chkUrl.toLowerCase().indexOf("/recruit/") > -1) {	//정직원
            settingPx = "-17px 0 0 400px;";
        } else if (chkUrl.toLowerCase().indexOf("/job/area/") > -1) {	//지역별
            settingPx = "-41px 0 0 300px;";
        } else if (chkUrl.toLowerCase().indexOf("/job/period/short") > -1) {	//단기알바
            settingPx = "-30px 0 0 400px;";
        }

        if (uid != "") {
            var strHTML;
            try {
                var strMyAdObjB = $(".B_MyAd_" + uid);	//배너형
                if (strMyAdObjB.length > 0) {
                    for (var i = 0; i < strMyAdObjB.length; i++) {
                        if ("False" == "True") {
                            strHTML = "<div onclick='javascript:hiddenMyAdLayers(this);' class='hiddenMyAdLayers hiddenMyAdLayersBrand'>";
                            strHTML += "<img src='http://image.alba.kr/job/c_pointer.png' alt='내가 등록한 채용공고' />";
                            strHTML += "<\/div>";
                        } else {
                            strHTML = "<div onclick='javascript:hiddenMyAdLayers(this);' class='hiddenMyAdLayers'>";
                            strHTML += "<img src='http://image.alba.kr/job/c_pointer.png' alt='내가 등록한 채용공고' />";
                            strHTML += "<\/div>";
                        }

                        $(strMyAdObjB[i]).html(strHTML);
                    }
                }
            } catch (e) { }

            try {
                var strMyAdObjL = $(".L_MyAd_" + uid);	//리스트형
                if (strMyAdObjL.length > 0) {
                    for (var i = 0; i < strMyAdObjL.length; i++) {
                        strHTML = "<div onclick='javascript:hiddenMyAdLayers(this);' style='z-index:1;position:absolute; margin:" + settingPx + " cursor:pointer;' >";
                        strHTML += "<img src='http://image.alba.kr/job/c_pointer.png' alt='내가 등록한 채용공고' />";
                        strHTML += "<\/div>";

                        $(strMyAdObjL[i]).html(strHTML);
                    }
                }
            } catch (e) { }
        }

    }

    function hiddenMyAdLayers(obj) {
        obj.style.visibility = "hidden";
    }
//]]>
</script>

