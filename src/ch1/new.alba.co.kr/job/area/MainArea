                    <script type="text/javascript">
                        gnbFunc.setDefaultKeyword();
                        $(".searchInput input").each(function () {
                            if ($(this).val() == "") {
                                $(this).siblings("label").show();
                            } else {
                                $(this).siblings("label").hide();
                            }
                        });
                        $(".wordSearch>input").focusin(function () {
                            $(this).siblings("label").hide();
                        }).focusout(function () {
                            if ($(this).val() == "") {
                                $(this).siblings("label").show();
                            } else {
                                $(this).siblings("label").hide();
                            }
                        });
                    </script>

