                        //<![CDATA[
                        //브랜드 알바
                        // 오픈중인 브랜드사이트 로딩
                        var arrBrandSite = BrandSiteArray["BRANDSITE"];
                        var strBrandSite = "";
                        var rndIdxBrand;
                        var intBrandPageSize = 9;
                        var intTotalCnt = arrBrandSite.length;
                        var intLineCnt = Math.ceil(intTotalCnt / 11);

                        if (intTotalCnt > 0) {
                            var rndIdxBrand = Math.floor(Math.random() * intTotalCnt);
                            strBrandSite += "<ul class='headerMenuFull-section-brandList'>";

                            // PMS-21590 할리스 고정위치 삭제
                            //strBrandSite += "<li><a href='http://hollys.alba.co.kr'' onclick=\"clickBrandSiteBanner('B94', 'RECOM');\"' target='_blank'><span><img src='//image.alba.kr/e/main/brandsite/logo_B94.png' alt='할리스' /></span>할리스</a></li>";

                            for (var i = 0; i < intTotalCnt; i++) {
                                var providercd = arrBrandSite[rndIdxBrand].PROVIDERCD;
                                providercd = providercd.toUpperCase();
                                var sitedomain = arrBrandSite[rndIdxBrand].SITEDOMAIN;
                                var brandsitenm = arrBrandSite[rndIdxBrand].BRANDSITENM;
                                var brandsitelogo = arrBrandSite[rndIdxBrand].BRANDSITELOGO;

                                // PMS-21590 할리스 고정위치 삭제
                                //if(providercd != 'B94'){
                                //	strBrandSite += "<li><a href='http://" + sitedomain + "' onclick=\"clickBrandSiteBanner('"+ providercd + "', 'RECOM');\"' target='_blank'><span><img src='//image.alba.kr/e/main/brandsite/logo_" + providercd + ".png' alt='" + brandsitenm + "' /></span>" + brandsitenm + "</a></li>";
                                //}
                                strBrandSite += "<li><a href='http://" + sitedomain + "' onclick=\"clickBrandSiteBanner('" + providercd + "', 'RECOM');\"' target='_blank'><span><img src='//image.alba.kr/e/main/brandsite/logo_" + providercd + ".png' alt='" + brandsitenm + "' /></span>" + brandsitenm + "</a></li>";

                                if (rndIdxBrand == (intTotalCnt - 1)) {
                                    rndIdxBrand = 0;
                                } else {
                                    rndIdxBrand++;
                                }
                            }
                            strBrandSite += "</ul>";
                            $("#BrandGlobal").html(strBrandSite);
                        }
