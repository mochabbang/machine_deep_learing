<script type="text/javascript">
    var BAEvent = {
        Collect: function (param) {
            var category1 = ((param.category1 != "" && typeof (param.category1) != "undefined") ? param.category1 : "");
            var category2 = ((param.action != "" && typeof (param.action) != "undefined") ? param.action : "");	// category2 : 액션명
            var category3 = ((param.category3 != "" && typeof (param.category3) != "undefined") ? param.category3 : "");
            var pageurl = ((param.pageurl != "" && typeof (param.pageurl) != "undefined") ? param.pageurl : "");
            var postdata = ((param.data != "" && typeof (param.data) != "undefined") ? param.data : "");

            if (param.action != "" && typeof (param.action) != "undefined") {
                switch (param.action) {
                    case "hr_scrap": pageurl = "/biz/customized/ResumeScrapInsProc.asp"; category1 = "인재스크랩"; break;
                    case "custom_hr_complete": pageurl = "/biz/customized/Customizing_reg_ok.asp"; category1 = "맞춤인재설정실행"; break;
                    case "jobinfo_apply_call": pageurl = ""; category1 = "전화지원"; break;
                    case "jobinfo_apply_email": pageurl = ""; category1 = "이메일지원"; break;
                    case "jobinfo_apply_simple": pageurl = ""; category1 = "간편지원"; break;
                    case "jobinfo_apply_homepage": pageurl = ""; category1 = "홈페이지지원"; break;
                    case "jobinfo_apply_online": pageurl = ""; category1 = "온라인지원"; break;
                    case "jobinfo_apply_sms": pageurl = ""; category1 = "문자지원"; break;
                    case "jobinfo_apply_talk": pageurl = ""; category1 = "문자지원 알림톡"; break;
                    case "jobinfo_apply_chat": pageurl = ""; category1 = "채팅문의&지원"; break;
                    case "jobinfo_apply_findjob": pageurl = ""; category1 = "벼룩시장지원"; break;
                    case "profile_try_safetynumber": pageurl = "/person/resume/Regist"; category1 = "회원가입후 이력서작성시도"; break;
                    case "profile_safetynumber": pageurl = "/person/resume/ResumeComplete.asp"; category1 = "본인인증안함 인재정보공개"; break;
                    case "pushoptin_safetynumber": pageurl = "/person/resume/ResumeComplete.asp"; category1 = "본인인증후 인재정보공개"; break;
                    case "goods_select": pageurl = "/biz/job/Regist.asp" + param.getdata; category1 = "상품선택"; category2 = "상품추천"; break;
                    case "online_apply_cancel": pageurl = "/person/online/ApplicationDeleteProc.asp"; category1 = "온라인입사지원 취소"; break;
                    case "email_apply_cancel": pageurl = "/person/email/ApplicationDeleteProc.asp"; category1 = "이메일입사지원 삭제"; break;
                    case "sms_apply_cancel": pageurl = "/person/online/ApplicationDeleteProc.asp"; category1 = "문자지원 삭제"; break;
                    case "talk_apply_cancel": pageurl = "/person/online/ApplicationDeleteProc.asp"; category1 = "문자지원 알림톡 삭제"; break;
                    case "simple_apply_cancel": pageurl = "/person/online/ApplicationDeleteProc.asp"; category1 = "간편입사지원 삭제"; break;
                    case "call_apply_cancel": pageurl = "/person/online/ApplicationDeleteProc.asp"; category1 = "전화지원 삭제"; break;
                    case "homepage_apply_cancel": pageurl = "/person/online/ApplicationDeleteProc.asp"; category1 = "홈페이지지원 삭제"; break;
                    default: pageurl = ""; category1 = "";
                }
                if(param.action.indexOf("jobinfo_apply") > -1) {
                    gtag('event', category2);
                }
            }

            BA.Collect("EVENT", {
                "category1": category1
                , "category2": category2
                , "category3": category3
                , "pageurl": pageurl
                , "postdata": postdata
            });
        }
    }
</script>



