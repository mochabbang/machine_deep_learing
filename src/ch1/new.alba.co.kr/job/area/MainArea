<!-- Footer -->
<script type="text/javascript">
    //var container = $('#SubContents');
    //var guideBox = $('.guide__box');
    var guideLayerWrapper = $('.guide__filter, .guide__layer');
    var guideFrame = $('.guide__frame');
    var flagURL = (window.location.pathname).toLowerCase();
    var eventkey, eventurl, actionnm;

    eventkey = "";
    eventurl = "";
    actionnm = "";

    //container.append(guideBox)
    function guideFrameShow() {

        switch (flagURL) {
            case "/biz/job/regist.asp": eventkey = "regist"; eventurl = "https://www.alba.co.kr/biz/job/Regist.asp"; actionnm = "공고입력"; break;
            case "/biz/jobsimple/registsimple.asp": eventkey = "jobsimple"; eventurl = "https://www.alba.co.kr/biz/jobsimple/RegistSimple.asp"; actionnm = "간편공고등록"; break;
            case "/biz/job/modify.asp": eventkey = "modify"; eventurl = "https://www.alba.co.kr/biz/job/Modify.asp"; actionnm = "채용공고수정"; break;
            case "/biz/job/manage": eventkey = "manage"; eventurl = "https://www.alba.co.kr/biz/job/Manage"; actionnm = "공고관리리스트"; break;
            case "/biz/resume/readingbreakdown": eventkey = "profile"; eventurl = "https://www.alba.co.kr/biz/resume/ReadingBreakDown"; actionnm = "이력서열람"; break;
            case "/biz/customized/customized": eventkey = "customized"; eventurl = "https://www.alba.co.kr/biz/customized/Customized"; actionnm = "맞춤인재"; break;
            case "/resume/main.asp": eventkey = "resume"; eventurl = "https://www.alba.co.kr/resume/Main.asp"; actionnm = "인재정보 홈"; break;
            case "/biz/main": eventkey = "biz"; eventurl = "https://www.alba.co.kr/biz/Main"; actionnm = "기업 홈"; break;
            default: break;
        }

        console.log("=>" + eventurl);

        if (flagURL === "/biz/job/regist.asp" || flagURL === "/biz/jobsimple/registsimple.asp" || flagURL === "/biz/job/modify.asp") {
            guideLayerWrapper.show();
            guideFrame.attr('src', '/biz/RegistJobGuide.asp');
        } else if (flagURL === "/biz/job/manage.asp" || flagURL === "/biz/job/manage" || flagURL === "/biz/resume/readingbreakdown.asp" || flagURL === "/biz/resume/readingbreakdown" || flagURL === "/biz/customized/customized.asp" || flagURL === "/biz/customized/customized" || flagURL === "/resume/main.asp") {
            guideLayerWrapper.show();
            guideFrame.attr('src', '/biz/SearchTalentGuide.asp');
        } else if (flagURL === "/biz/main") {
            guideLayerWrapper.show();
            if ($(event.target).is('.main__tip-action--type1')) {
                guideFrame.attr('src', '/biz/SearchTalentGuide.asp');
            } else if ($(event.target).is('.main__tip-action--type2')) {
                guideFrame.attr('src', '/biz/RegistJobGuide.asp');
            }
        }
    }

    var eventMethod = window.addEventListener ? "addEventListener" : "attachEvent";
    var eventer = window[eventMethod];
    var messageEvent = eventMethod == "attachEvent" ? "onmessage" : "message";

    eventer(messageEvent, function (e) {
        if (e.origin != 'https://new.alba.co.kr') {
            return;
        }

        if (e.data == 'open_event_tip1') {
            var category1, category2;
            category1 = "공고등록 TIP 조회";
            category2 = "tip1_" + eventkey + "_view";

            sendEvent(category1, category2)
        }

        if (e.data == 'open_event_tip2') {
            var category1, category2;
            category1 = "인재찾기 TIP 조회";
            category2 = "tip2_" + eventkey + "_view";

            sendEvent(category1, category2)
        }

        if (e.data == 'coupon_event_tip1') {
            var category1, category2;
            category1 = "공고등록 TIP 쿠폰수령 완료";
            category2 = "tip1_" + eventkey + "_coupon";

            sendEvent(category1, category2)
        }

        if (e.data == 'coupon_event_tip2') {
            var category1, category2;
            category1 = "인재찾기 TIP 쿠폰수령 완료";
            category2 = "tip2_" + eventkey + "_coupon";

            sendEvent(category1, category2)
        }

        if (e.data == 'complete_event_tip1') {
            var category1, category2;
            category1 = "공고등록 TIP 확인완료";
            category2 = "tip1_" + eventkey + "_complete";

            sendEvent(category1, category2)
        }

        if (e.data == 'complete_event_tip2') {
            var category1, category2;
            category1 = "인재찾기 TIP 확인완료";
            category2 = "tip2_" + eventkey + "_complete";

            sendEvent(category1, category2)
        }

        if (e.data == 'guide_frame_close') {
            guideFrameHide();
        }
    }, false);

    function sendEvent(category1, category2) {
        BA.Collect("EVENT", {
            "pageurl": eventurl
            , "category1": category1
            , "category2": category2
        });
    }

    function guideFrameHide() {
        guideLayerWrapper.hide();
        guideFrame.attr('src', '');
    }
</script>


