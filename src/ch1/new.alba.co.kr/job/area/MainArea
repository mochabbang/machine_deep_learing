<script type="text/javascript" src="/rsc/js/CommonCtrl_min.js?20240202083626"></script>
<script type="text/javascript">
    //<![CDATA[
    var _commonctrl = new CommonCtrl('');
    var iListIdx = 0;
    var skyBanner = {

        formatNumberSkyBanner: function (num) {
            /*	ver2####*/
            var reg = /(^[+-]?\d+)(\d{3})/;
            num = Number(num)
            num = String(num)

            while (reg.test(num))
                num = num.replace(reg, '$1' + ',' + '$2');

            return num;
        },

        // 오늘 본 공고
        clsReadedRcList: function (adid, title, comnm, areanm, address1, paytype, pay) {
            this.adid = adid;
            this.title = title;
            this.comnm = comnm;
            this.areanm = areanm;
            this.address1 = address1;
            this.paytype = paytype;
            this.pay = pay;
        },

        // 오늘 본 인재
        clsReadedIjList: function (adid, usernm, gender, age, title, addr1) {
            this.adid = adid;
            this.usernm = usernm;
            this.gender = gender;
            this.age = age;
            this.title = title;
            this.addr1 = addr1;
        },

        // 페이징
        pageList: function (param) {
            var strType = _commonctrl.isUndefined(param.type) ? param.type : "";
            var strValue = _commonctrl.isUndefined(param.value) ? param.value : "";

            var totalPage = 1;
            if (strType == "rclist") {
                if (iListRcCnt % 3 == 0) {
                    totalPage = Math.floor(iListRcCnt / 3);
                } else {
                    totalPage = Math.floor(iListRcCnt / 3) + 1;
                }
            } else {
                if (iListIjCnt % 3 == 0) {
                    totalPage = Math.floor(iListIjCnt / 3);
                } else {
                    totalPage = Math.floor(iListIjCnt / 3) + 1;
                }
            }
            $("#TotalPageNum").html(totalPage);

            var nowPageNum = $("#NowPageNum").html();
            var totalPageNum = $("#TotalPageNum").html();

            if (strValue == "prev") {
                if (nowPageNum == "1" || totalPageNum == "0") {
                    if (strType == "rclist") {
                        alert("이전에 본 채용공고가 없습니다.");
                    } else {
                        alert("이전에 열람하신 인재정보가 없습니다.");
                    }
                    return false;
                } else {
                    iListIdx = iListIdx - 3;
                    $("#NowPageNum").html(Number(nowPageNum) - 1);
                }
            }
            if (strValue == "next") {
                if (nowPageNum == totalPageNum || totalPageNum == "0") {
                    if (strType == "rclist") {
                        alert("다음에 본 채용공고가 없습니다.");
                    } else {
                        alert("다음에 열람하신 인재정보가 없습니다.");
                    }
                    return false;
                } else {
                    iListIdx = iListIdx + 3;
                    $("#NowPageNum").html(Number(nowPageNum) + 1);
                }
            }

            var j = iListIdx;
            if (strType == "rclist") {
                if (iListRcCnt > 3) {
                    for (var i = 0; i < 3; i++) {
                        if (j > iListRcCnt - 1) j = 0;
                        document.getElementById("ReadRcList_" + i).innerHTML = '<a href="/job/Detail?adid=' + arrReadedRcList[j].adid + '"><strong>' + arrReadedRcList[j].comnm + '<\/strong>' + arrReadedRcList[j].title + '<span class="local">' + arrReadedRcList[j].areanm + '/' + arrReadedRcList[j].address1 + '<\/span><span class="pay">' + arrReadedRcList[j].paytype + (isNaN(arrReadedRcList[j].pay) ? '<span class="consult">' + arrReadedRcList[j].pay + '<\/span>' : '<span class="num">' + skyBanner.formatNumberSkyBanner(arrReadedRcList[j].pay) + '<\/span>원') + '</span><\/a>';
                        j++;
                    }
                }
            } else {
                if (iListIjCnt > 3) {
                    for (var i = 0; i < 3; i++) {
                        if (j > iListIjCnt - 1) j = 0;
                        document.getElementById("ReadIjList_" + i).innerHTML = '<a href="/resume/Detail?adid=' + arrReadedIjList[j].adid + '"><strong>' + arrReadedIjList[j].usernm + ' (' + arrReadedIjList[j].gender + ',' + arrReadedIjList[j].age + ')<\/strong>' + arrReadedIjList[j].title + '<span class="local">' + arrReadedIjList[j].addr1 + '<\/span><\/a>';
                        j++;
                    }
                }
            }
        }

    };

    var arrReadedRcList = new Array();
    
    var iListRcCnt = arrReadedRcList.length;

    var arrReadedIjList = new Array();
    
    var iListIjCnt = arrReadedIjList.length;
    //]]>
</script>

